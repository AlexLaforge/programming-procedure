$('.cbRelationship').change(function () {
    if ($(this).is(":checked")) {
        addColumnsToTable();

    }
});


function addColumnsToTable() {
    //mang-content-table
    $('.mang-content-table tr').append($("<td>"));
    $('.mang-content-table thead tr>td:last').html('new col'); //<--- add column title
    $('.mang-content-table tbody tr').each(function () {
        $(this).children('td:last').append($('<input type="checkbox">'));
    });
}
